{
  "name": "benshada",
  "version": "0.1.0",
  "description": "African fashion without borders",
  "keywords": [
    "Africa",
    "fashion",
    "benshada",
    "benshada place"
  ],
  "private": false,
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "test": "cross-env CI=true react-scripts test --detectOpenHandles",
    "test:watch": "react-scripts test --watch",
    "test:cover": "react-scripts test --coverage",
    "lint": "eslint \"src/**/*.js\" --fix || false",
    "check": "npm run lint && npm test",
    "build": "npm run check && react-scripts build",
    "start": "serve -s build",
    "dev": "npm run check && set PORT=3004 && react-scripts start",
    "dev:secure": "npm run check && set PORT=3003 && cross-env HTTPS=true react-scripts start",
    "prod": "npm run build && serve -s build",
    "aws": "npm run build && aws s3 cp build s3://benshada/ --recursive"
  },
  "pre-commit": "check",
  "author": {
    "name": "Aestart",
    "email": "aestartng@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AestArt/benshada-client.git"
  },
  "bugs": {
    "url": "https://github.com/AestArt/benshada-client/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.4.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.0",
    "cross-env": "^7.0.2",
    "d3": "5.16.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest-enzyme": "^7.1.2",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "luhn": "^2.4.1",
    "popper.js": "^1.16.1",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-bootstrap": "^1.0.1",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.13.1",
    "react-flutterwave-rave": "^1.0.4",
    "react-js-pagination": "^3.0.3",
    "react-loading-screen": "0.0.17",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-select": "^3.1.0",
    "react-toastify": "^6.0.5",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.3.6",
    "redux-localstorage-simple": "^2.2.0",
    "redux-multi": "^0.1.12",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "serve": "^11.3.1",
    "webpack": "4.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
